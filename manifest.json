{
    "name": "UoL Coursera Grades",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Shows staff-graded project grades on the calendar page",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "https://www.coursera.org/degrees/bachelor-of-science-computer-science-london/home",
        "tabs",
        "webRequest"
    ]
}